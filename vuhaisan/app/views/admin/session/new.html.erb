<h1> LOGIN</h1>
<hr/>

<%= form_tag admin_sessions_path, method: 'post' do %>
  <table>
    <tr>
      <td>
        Username:
      </td>
      <td>
        <%= text_field_tag :username %>
      </td>
    </tr>
    <tr>
      <td>
        Password:
      </td>
      <td>
        <%= password_field_tag :pass %>
      </td>
    </tr>
    <tr>
      <td></td>
      <td>
        <%= submit_tag "Submit" %>
      </td>
    </tr>
  </table>
<% end %>